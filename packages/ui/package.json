{
    "name": "cosmic-ui-alpha",
    "version": "0.0.16-cosmic-ui-alpha.0",
    "main": "./dist/index.umd.js",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.umd.js"
        },
        "./style.css": "./dist/style.css",
        "./token.css": "./dist/token.css",
        "./ui.css": "./dist/ui.css",
        "./utilities.css": "./dist/utilities.css"
    },
    "scripts": {
        "typecheck": "vue-tsc --noEmit -p ../../tsconfig.json",
        "build": "rimraf dist && npm run build:dts && npm run build:vite && npm run build:css",
        "build:vite": "vite build",
        "build:dts": "tcm -p './**/**.module.css'",
        "build:css": "cosmic-util -u ./dist/static-utilities.css && node ./script/build-css.js"
    },
    "devDependencies": {
        "cosmic-util": "workspace:*",
        "rimraf": "^3.0.2",
        "vite-dts": "^1.0.4"
    }
}
